<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="AnchorPane" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.catan.catanui.controller.MainController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="10" layoutX="250" layoutY="150">
        <Button text="Play" fx:id="playButton" onAction="#handlePlayButtonAction" />
        <Button text="Leaderboard" fx:id="leaderboardButton" onAction="#handleLeaderboardButtonAction" />
    </VBox>
</AnchorPane>
