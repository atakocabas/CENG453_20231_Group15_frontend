<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.catan.catanui.controller.ForgotPasswordController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" layoutX="250" layoutY="150">
        <Label text="Enter your username:" />
        <TextField fx:id="usernameField" />
        <Button text="Back" fx:id="backButton" onAction="#handleBackButtonAction" />
        <Button text="Submit" fx:id="submitButton" onAction="#handleSubmitButtonAction" />
    </VBox>

</AnchorPane>
